<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <!-- Cascading Stylesheets -->
    <link rel="stylesheet" href="/css/stocks/colors.css" />
    <link rel="stylesheet" href="/css/dashboard2.css" />
    <link rel="stylesheet" href="/css/dashboard_divs.css">

    <!-- Font Awesome -->
    <script src="https://kit.fontawesome.com/40989200a5.js" crossorigin="anonymous"></script>
    <script src="https://kit.fontawesome.com/1542790c88.js" crossorigin="anonymous"></script>


    <title>Dashboard 2</title>
</head>

<body>
    <div class="grid-container">
        <header>
            <h1>PORTFOLIO <br>MANAGER</h1>

            <div class="nav-container">
                <div class="page-container" id="dashboard">
                    <img src="../images/dashboard/dashboard.png" alt="">
                    <span >Dashboard</li>
                </div>
                <div class="page-container" id="profile">
                    <a href="/profile">
                        <img src="../images/dashboard/profile.png" alt="">
                        <span >Profile</span>
                    </a>
                </div>

                <div class="page-container" id="liability">
                    <a href="/liabilities">
                        <img src="../images/dashboard/liability icon 2.png" alt="">
                        <span > Liability</span>
                    </a>
                </div>

                <div class="page-container" id="insurance">
                    <a href="/insurance">
                        <img src="../images/dashboard/insurance.png" alt="">
                        <span > Insurance</span>
                    </a>
                </div>

                <div class="page-container" id="stocks">
                    <a href="/stocks">
                        <img src="../images/dashboard/stocks.png" alt="">
                        <span > Stocks</span>
                    </a>
                </div>

                <div class="page-container" id="crypto">
                    <a href="/crypto">
                        <img src="../images/dashboard/crypto.png" alt="">
                        <span > Crypto</span>
                    </a>
                </div>

                <div class="logout">
                    <a href="/logout">
                        <i class="fa-solid fa-power-off"></i><span>Logout</span>
                    </a>
                </div>
            </div>

        </header>

        <div class="data-container">
            <div id="user-data" class="data-box">
            </div>
            <div id="profile-data" class="data-box">
                <h2>
                    Profile
                </h2>
            </div>
            <div id="liability-data" class="data-box">
                <h2>
                    Liability
                </h2>
            </div>
            <div id="insurance-data" class="data-box">
                <h2>
                    Insurance
                </h2>
            </div>
            <div id="stocks-data" class="data-box">
                <h2>
                    Stocks
                </h2>
            </div>
            <div id="crypto-data" class="data-box">
                <h2>
                    Crypto
                </h2>
            </div>
        </div>

    </div>
    <script src="js/dashboard.js"></script>
</body>

</html>